                  "media_images": {
                    "content_images": [
                      {
                        "displayphoto": "https://www.jornada.com.mx/ndjsimg/images/jornada/jornadaimg/en-su-obra-alvarez-bravo-y-schalkwijk-tienden-un-puente-entre-mexico-y-holanda/en-su-obra-alvarez-bravo-y-schalkwijk-tienden-un-puente-entre-mexico-y-holanda_148882bd-1e3e-44b6-98f5-bd5060900581_media",
                        "dirbucket": "jornadaimg/en-su-obra-alvarez-bravo-y-schalkwijk-tienden-un-puente-entre-mexico-y-holanda/en-su-obra-alvarez-bravo-y-schalkwijk-tienden-un-puente-entre-mexico-y-holanda_148882bd-1e3e-44b6-98f5-bd5060900581_media",
                        "author": "Foto cortesía Casa Manuel Álvarez Bravo",
                        "descripcion": "Bob Schalkwijk cumplirá mañana 92 años. Aquí, una imagen de su autoría captada en San Juan Chamula, Chiapas, en 1960."
                      }
