                    "content_images": [
                      {
                        "displayphoto": "https://www.jornada.com.mx/ndjsimg/images/jornada/jornadaimg/star-wars-me-ha-dado-opciones-por-eso-pude-desarrollar-mi-carrera-daisy-ridley/star-wars-me-ha-dado-opciones-por-eso-pude-desarrollar-mi-carrera-daisy-ridley_adf89aa7-39e3-4853-83c8-a7b8ba25e567_media",
                        "dirbucket": "jornadaimg/star-wars-me-ha-dado-opciones-por-eso-pude-desarrollar-mi-carrera-daisy-ridley/star-wars-me-ha-dado-opciones-por-eso-pude-desarrollar-mi-carrera-daisy-ridley_adf89aa7-39e3-4853-83c8-a7b8ba25e567_media",
